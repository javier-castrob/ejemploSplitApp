{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run EjemploSplitApp-EjemploSplitApp",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/ejemplosplitapp-approuter/node_modules/@sap/html5-repo-mock/index.js",
      "args": [
        "--standalone",
        "/EjemploSplitAppEjemploSplitApp/index.html",
        "--ui5",
        "{\"version\":\"1.84.9\"}"
      ],
      "cwd": "${workspaceFolder}/EjemploSplitApp",
      "env": {
        "PORT": "6004",
        "MOCK_LOOKUP_DIRS": "[\"webapp\"]",
        "run.config": "{\"handlerId\":\"ui5_run_config_handler_id\",\"runnableId\":\"/home/user/projects/EjemploSplitApp/EjemploSplitApp\"}"
      },
      "envFile": "${workspaceFolder}/EjemploSplitApp/.env1",
      "preLaunchTask": "Build EjemploSplitApp"
    }
  ]
}